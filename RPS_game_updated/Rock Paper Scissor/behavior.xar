                            <script language="4">
                                <content>
                                    <![CDATA[# Import the requests library to send HTTP POST requests to the Flask server
import requests

# Import ALProxy to communicate with NAO's onboard modules (e.g. camera)
from naoqi import ALProxy

# Import NumPy for array manipulation (used to reshape image data)
import numpy as np

# Import OpenCV to encode image data into JPEG format before sending
import cv2

# Define a function to capture a camera frame from the NAO robot
def capture_image(ip, port):
    # Create a proxy to access NAO's video device
    camProxy = ALProxy("ALVideoDevice", ip, port)

    # Define image resolution (2 = VGA), color space (11 = RGB), and frame rate
    resolution = 2        # VGA: 640x480
    colorSpace = 11       # RGB color space
    fps = 5               # Frames per second (low FPS for single snapshot)

    # Subscribe to the camera and get an image
    nameId = camProxy.subscribe("rps_cam", resolution, colorSpace, fps)
    naoImage = camProxy.getImageRemote(nameId)
    camProxy.unsubscribe(nameId)  # Always unsubscribe to free up camera resources

    # Extract width, height, and pixel array from the image data
    width = naoImage[0]
    height = naoImage[1]
    array = naoImage[6]  # Raw byte data representing the image

    # Convert raw byte array to a NumPy array and reshape it to an image
    # Use fromstring and bytearray for Python 2.7 compatibility
    image = np.fromstring(bytearray(array), dtype=np.uint8).reshape((height, width, 3))

    # Encode the image as JPEG so it can be sent over HTTP
    success, img_encoded = cv2.imencode('.jpg', image)

    # Return the JPEG byte stream if successful, otherwise return None
    if success:
        return img_encoded.tostring()
    else:
        return None

# Main entry point when the box receives an 'onStart' input
def onInput_onStart():
    try:
        # Set the NAO robot's IP  '127.0.0.1' since this script runs on the robot itself
        nao_ip = "127.0.0.1"

        # Set the URL of the Flask server on your PC
        # Make sure to replace this with your actual PC's IP address
        server_url = "http://10.117.35.54:5000/classify"

        # Capture an image from the NAO camera
        image_data = capture_image(nao_ip, 9559)

        # If encoding failed, output an error message and stop
        if image_data is None:
            print "Error: Could not encode image"
            output("gestureDetected", "error")  # Send 'error' as gesture
            output("onStopped")  # Continue flow
            return

        # Prepare the image to send as a file in the HTTP POST request
        files = {'image': ('nao_image.jpg', image_data, 'image/jpeg')}

        # Send the POST request to the Flask server
        response = requests.post(server_url, files=files)

        # Parse the JSON response to get the detected gesture
        gesture = response.json().get('gesture', 'unknown')

        # Print the gesture result for debugging
        print "Detected gesture:", gesture

        # Output the gesture to the next box in the behavior flow
        output("gestureDetected", gesture)

    except Exception as e:
        # Catch and print any error that occurs (e.g. network error, invalid image)
        print "Exception during gesture detection:", str(e)
        output("gestureDetected", "error")  # Output 'error' if something goes wrong

    # Continue the behavior flow (use built-in output)
    output("onStopped")]]>
</content>
